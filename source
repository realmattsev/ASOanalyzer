        <li class="inline-list__item inline-list__item--margin-inline-end-large">
          <a class="link icon icon-after icon-external" data-metrics-click="{&quot;actionType&quot;:&quot;navigate&quot;,&quot;targetType&quot;:&quot;link&quot;,&quot;targetId&quot;:&quot;LinkToPrivacyPolicy&quot;}" href="https://mindnation.com/privacy-policy?lang=en">
            Privacy Policy
          </a>
        </li>
<!----><!---->    </ul>
  </div>
</section>

